{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "target": "es6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "baseUrl": "./src",
    "typeRoots": ["node_modules/@types", "src/types"],
    "skipLibCheck": true,
  }
}
